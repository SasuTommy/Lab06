package javaapplication3;

public class Statek implements plywa{
    double predkosc;
    
    public double getPredkosc(){
    return predkosc;
    }
    public void setPredkosc(double predkosc){
    this.predkosc = predkosc;
    }
    
    
    public void plyn(){
    System.out.println("Statek plynie z predkoscia: "+getPredkosc()+" km");
    }
    
}
